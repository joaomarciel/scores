let score = 90    // insert your score here

// score more than 90 = A
if(score > 90) {
    console.log('A')

// score between 80 and 89 = B
} else if (score >= 80 && score <= 89) {
    console.log('B')

// score between 70 and 79 = C
} else if (score >= 70 && score <= 79) {
    console.log('C')

// score between 60 and 69 = D
} else if (score >= 60 && score <= 69) {
    console.log('D')

// score less than 60 = F
} else {
    console.log('F')
}
